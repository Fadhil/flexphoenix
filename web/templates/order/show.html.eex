<div class="row">
  <div class="col-lg-9">
    <div class="wrapper wrapper-content animated fadeInUp">
      <div class="ibox">
        <div class="ibox-content">
          <div class="row">
            <div class="col-lg-12">
              <div class="m-b-md">
                <%= link "Back", to: order_path(@conn, :index),
                    class: "btn btn-default btn-outline btn-rounded btn-sm pull-right" %>
                <%= link "Edit Order", to: order_path(@conn, :edit, @order),
                    class: "btn btn-warning btn-rounded btn-sm pull-right" %>

                <h2><%= @order.title %></h2>
              </div>
              <dl class="dl-horizontal">
                <dt>Request ID:</dt>
                <dd>
                  <span class="label label-sucess">
                    <%= @order.request_id %>
                  </span>
                </dd>
                <dt>Location:</dt>
                <dd>
                  <span>
                    <%= @order.location %>
                  </span>
                </dd>
                <dt>Description</dt>
                <dd>
                  <span>
                    <%= @order.description %>
                  </span>
                </dd>
              </dl>
            </div>
          </div>
          <div class="row">
            <div class="col-lg-6">
              <dl class="dl-horizontal">

                <dt>Owner:</dt> <dd><%= display_name(@order.user) %></dd>
                <dt>Technicians:</dt>
                <dd class="project-people">
                  <%= for member <- @order.technicians do %>
                  <a href="" ><img alt="image" class="img-circle"
                          src="/images/default_profile_picture.png"
                          data-toggle="tooltip" data-placement="top"
                          title="<%= display_name(member) %>"></a>
                  <% end %>
                </dd>
              </dl>
            </div>


            <div class="col-lg-6" id="cluster_info">
              <dl class="dl-horizontal" >

                <dt>Last Updated:</dt> <dd><%= @order.updated_at %></dd>
                <dt>Created:</dt> <dd><%= @order.inserted_at %> </dd>

              </dl>
            </div>
          </div>
          <div class="row">
            <div class="col-lg-12">
              <dl class="dl-horizontal">
                <!--// ASsign Technician -->
                <%= link "Assign Technicians", to: order_assign_technicians_path(@conn, :assign_technicians, @order.id), class: "btn btn-primary btn-rounded" %>
              </dl>
            </div>
          </div>
          <div class="row">
            <div class="col-lg-12">
              <dl class="dl-horizontal">
                <dd>

                  <%= link "Create Work Report", to: report_path(@conn, :new, order_id: @order), class: "btn btn-success btn-outline btn-rounded btn-sm" %>
                </dd>
              </dl>
            </div>
          </div>

