
<div class="content">
  <div class="container-fluid">
    <div class="col-md-12">
      <div class="card">
        <div class="header">
          <h4 class="title">Add Asset to <%= @site.name %></h4>
        </div>
        <%= render "form.html", changeset: @changeset,
            action: organisation_site_asset_path(@conn, :create, @conn.assigns.organisation, @site),
            conn: @conn,
            site: @site,
            assets: @assets
          %>
      </div>
    </div>
  </div>
</div>
